    "@electric-sql/pglite-sync": "^0.2.18",
    "@mason/env": "workspace:*",
    "@supabase/ssr": "^0.5.2",
    "drizzle-orm": "^0.38.2",
    "postgres": "^3.4.5",
    "react": "^18.3.1",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@mason/config": "workspace:*",
    "@types/pg": "^8.11.10",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.1",
    "typescript": "^5.3.3"
  },
  "exports": {
